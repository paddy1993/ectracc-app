{
  "name": "ectracc-automated-testing",
  "version": "1.0.0",
  "description": "Comprehensive automated testing framework for ECTRACC application",
  "main": "continuous-testing.js",
  "scripts": {
    "test:smoke": "node smoke-tests.js",
    "test:sanity": "node sanity-tests.js",
    "test:regression": "node regression-tests.js",
    "test:continuous": "node continuous-testing.js",
    "test:black-box": "node black-box-tests.js",
    "test:white-box": "node white-box-tests.js",
    "test:gray-box": "node gray-box-tests.js",
    "test:api": "node api-tests.js",
    "test:mobile": "node mobile-tests.js",
    "test:all": "node continuous-testing.js --test-suite=full",
    "test:parallel": "node continuous-testing.js --test-suite=full --parallel=true",
    "test:ci": "node continuous-testing.js --trigger-type=commit --test-suite=smoke",
    "test:deploy": "node continuous-testing.js --trigger-type=deploy --test-suite=full",
    "test:box-testing": "npm run test:black-box && npm run test:white-box && npm run test:gray-box",
    "test:api-mobile": "npm run test:api && npm run test:mobile"
  },
  "keywords": [
    "testing",
    "automation",
    "regression",
    "smoke",
    "sanity",
    "continuous-integration",
    "ci-cd"
  ],
  "author": "ECTRACC Team",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  }
}
